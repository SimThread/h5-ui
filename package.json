{
  "name": "@hk591/h5-ui",
  "version": "0.0.64",
  "description": "hk591站台UI组件库，基于vant改造",
  "main": "lib/index.js",
  "module": "es/index.js",
  "style": "lib/index.css",
  "typings": "types/index.d.ts",
  "files": [
    "es",
    "lib",
    "src",
    "types",
    "vetur"
  ],
  "scripts": {
    "bootstrap": "npm i",
    "dev": "npm run build:entry && webpack-dev-server --inline --progress --config build/webpack.dev.js",
    "lint": "eslint --fix --ext .js,ts,.vue --ignore-path .eslintignore . && stylelint **/*.{css,less,vue} --fix --ignore-path  .eslintignore",
    "build:entry": "node build/build-entry.js",
    "build:components": "node build/build-components.js --color",
    "build:h5": "cross-env NODE_ENV=production webpack --color --config build/webpack.build.js && cross-env NODE_ENV=production webpack -p --color --config build/webpack.build.js",
    "build:style": "gulp --gulpfile ./build/build-style.js",
    "build:style-entry": "node build/build-style-entry.js",
    "build:lib": "node build/build-lib.js",
    "build:site": "rimraf docs/dist && cross-env NODE_ENV=production webpack --config build/webpack.doc.js",
    "release": "sh build/release.sh"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,vue}": [
      "eslint --fix --ignore-path .eslintignore .",
      "git add"
    ],
    "*.{css,less,vue}": [
      "stylelint **/*.{vue,less} --fix --ignore-path .eslintignore",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "http://code.addcn.com/hk591/h5-ui.git"
  },
  "keywords": [
    "vue",
    "component"
  ],
  "author": "chenxiong",
  "license": "MIT",
  "dependencies": {
    "@babel/runtime": "^7.3.1",
    "@vant/icons": "1.0.9",
    "@vue/babel-helper-vue-jsx-merge-props": "^1.0.0-beta.2",
    "axios": "^0.19.0",
    "better-scroll": "^1.15.2",
    "lottie-web": "^5.7.4",
    "nprogress": "^0.2.0",
    "swiper": "^5.3.8",
    "vue-lazyload": "1.2.3",
    "vue-range-component": "^1.0.3"
  },
  "peerDependencies": {
    "vue": ">= 2.5.0"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-optional-chaining": "^7.12.1",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-syntax-jsx": "^7.2.0",
    "@babel/plugin-transform-object-assign": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.1.0",
    "@babel/polyfill": "^7.2.5",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-typescript": "^7.1.0",
    "@typescript-eslint/eslint-plugin": "^2.13.0",
    "@typescript-eslint/parser": "^2.13.0",
    "@vant/markdown-loader": "^1.0.3",
    "@vue/babel-preset-jsx": "^1.0.0-beta.2",
    "autoprefixer": "^9.4.7",
    "babel-loader": "^8.0.4",
    "codecov": "^3.1.0",
    "copy-webpack-plugin": "^5.1.1",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.0",
    "dependency-tree": "^7.0.2",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-vue": "^6.0.2",
    "fast-glob": "^2.2.4",
    "file-loader": "^4.2.0",
    "fs-extra": "^7.0.1",
    "gulp": "3.9.1",
    "gulp-csso": "^3.0.1",
    "gulp-debug": "^4.0.0",
    "gulp-less": "^4.0.1",
    "gulp-postcss": "^8.0.0",
    "html-webpack-plugin": "3.2.0",
    "husky": "^1.3.1",
    "less": "^3.8.1",
    "less-loader": "^4.1.0",
    "lint-staged": "^8.1.3",
    "markdown-vetur": "0.0.5",
    "postcss": "^7.0.14",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.19.1",
    "progress-bar-webpack-plugin": "^1.12.1",
    "rimraf": "^2.5.4",
    "shelljs": "^0.8.3",
    "signale": "^1.4.0",
    "style-loader": "^0.23.1",
    "stylelint": "^9.10.1",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-scss": "^3.13.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "svg-sprite-loader": "^5.0.0",
    "typescript": "^3.3.3",
    "uppercamelcase": "^3.0.0",
    "url-loader": "^1.1.2",
    "vue": "2.6.6",
    "vue-loader": "^15.6.2",
    "vue-router": "^3.0.2",
    "vue-server-renderer": "^2.6.6",
    "vue-template-compiler": "2.6.6",
    "vue-template-es2015-compiler": "^1.8.2",
    "webpack": "^4.29.3",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3"
  },
  "vetur": {
    "tags": "vetur/tags.json",
    "attributes": "vetur/attributes.json"
  }
}
